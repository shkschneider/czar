// https://github.com/TinyCC/examples/ex3.c

#include <stdlib.h>
#include <stdio.h>

u32 fib(u32 n) {
    if (n <= 2) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

u32 main(int argc, char **argv) {
    if (argc < 2) {
        return 1;
    }
    for (mut u32 i = 1; i < argc; i++) {
        u32 n = atoi(argv[i]);
        printf("fib(%d) = %d\n", n, fib(n));
    }
    return 0;
}
