// print.cz - Print functions for formatted output
// Part of the Czar standard library
//
// Note: print, println, printf are function pointers in the cz struct
// They point to raw C functions _cz_print, _cz_println, _cz_printf
#module cz

pub fn print(string fmt, any... args) {
    _cz_print(fmt:cstr(), args)
}

pub fn printf(string fmt, any... args) {
    _cz_printf(fmt:cstr(), args)
}

pub fn println(string fmt, any... args) {
    _cz_println(fmt:cstr(), args)
}
