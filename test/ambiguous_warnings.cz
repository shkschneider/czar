#include <stdio.h>

void my_function(u8 x, u8 y) {
    printf("x=%d, y=%d\n", x, y);
}

u32 main(void) {
    /* Test 1: Fixed - use labels to avoid error */
    my_function(x = 2, y = 3);

    /* Test 2: Should NOT error - first labeled */
    my_function(x = 2, 3);

    /* Test 3: Should NOT error - both labeled */
    my_function(x = 2, y = 3);

    /* Test 4: Should NOT error - last labeled */
    my_function(2, y = 3);

    return 0;
}
