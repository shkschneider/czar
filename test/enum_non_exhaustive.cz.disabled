#include <stdint.h>
#include <stddef.h>
#include <stdio.h>

/* Test enum exhaustiveness checking - should fail */

enum Color {
    COLOR_RED,
    COLOR_GREEN,
    COLOR_BLUE
};

const char* color_name(enum Color c) {
    switch (c) {
    case COLOR_RED:
        return "Red";
    case COLOR_GREEN:
        return "Green";
    /* Missing COLOR_BLUE case - should error! */
    }
    return "Unknown";
}

int main(void) {
    enum Color c = COLOR_RED;
    printf("Color: %s\n", color_name(c));
    return 0;
}
