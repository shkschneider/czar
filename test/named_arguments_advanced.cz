#include <stdio.h>

/* Test with different types and more parameters */
void complex_func(u8 a, u16 b, u32 c, i8 d, i16 e) {
    printf("a=%d, b=%d, c=%d, d=%d, e=%d\n", a, b, c, d, e);
}

/* Test with pointers */
void pointer_func(mut u8 *ptr, u8 value) {
    *ptr = value;
}

u32 main(void) {
    /* Test 1: Mix of named and positional */
    complex_func(a = 1, 2, 3, d = 4, 5);

    /* Test 2: All positional */
    complex_func(10, 20, 30, 40, 50);

    /* Test 3: All named in order */
    complex_func(a = 11, b = 22, c = 33, d = 44, e = 55);

    /* Test 4: Pointer function with named args */
    mut u8 x = 0;
    pointer_func(ptr = &x, value = 42);
    printf("x=%d\n", x);

    /* Test 5: Nested function calls - must use labels for same base type */
    mut u8 y = 0;
    pointer_func(ptr = &y, value = 99);
    printf("y=%d\n", y);

    return 0;
}
