#include <stdint.h>
#include <stddef.h>
#include <stdio.h>

/* Test CZar type replacements (no cz.h needed for types) */

int main(void) {
    u8 a = 100;
    u16 b = 50000;
    i32 c = -12345;
    f32 d = 3.14f;

    printf("Types test: ");
    printf("u8=%u, u16=%u, i32=%d, f32=%.2f\n", a, b, c, d);

    printf("Limits test: ");
    printf("U8_MAX=%u, I32_MIN=%d, I32_MAX=%d\n",
           U8_MAX, I32_MIN, I32_MAX);

    printf("CZar types: OK\n");
    return 0;
}
