// Test arithmetic operators
fn main() -> i32 {
    val i32 a = 10
    val i32 b = 3
    
    val i32 sum = a + b        // 13
    val i32 diff = a - b       // 7
    val i32 prod = a * b       // 30
    val i32 quot = a / b       // 3
    
    return sum + diff + prod + quot  // 13 + 7 + 30 + 3 = 53
}
