#include <stdio.h>
#include <stdint.h>

/*
 * Test that C-style casts produce errors
 * This test should FAIL to compile
 */

int main(void) {
    u32 value = 1000;
    
    /* This should ERROR - C-style cast not allowed */
    u8 narrow = (u8)value;
    
    printf("Value: %u\n", narrow);
    return 0;
}
