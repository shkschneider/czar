// Test comments: single-line // and multi-line /* */
fn main() -> i32 {
    // This is a single-line comment
    x: i32 = 10
    
    /* This is a 
       multi-line
       block comment */
    y: i32 = 20
    
    /* Inline block comment */ z: i32 = 30
    
    return x + y + z  // inline comment - should return 60
}
