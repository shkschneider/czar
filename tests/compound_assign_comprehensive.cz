// Comprehensive test for all compound assignment operators
fn main() -> i32 {
    var a: i32 = 100
    var b: i32 = 50
    var c: i32 = 10
    var d: i32 = 20
    
    // Test all operators
    a += 50   // 150
    b -= 10   // 40
    c *= 5    // 50
    d /= 2    // 10
    
    // Combined result
    var result: i32 = a + b + c + d  // 150 + 40 + 50 + 10 = 250
    
    return result
}
