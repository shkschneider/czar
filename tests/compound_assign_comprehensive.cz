// Comprehensive test for all compound assignment operators
fn main() -> i32 {
i32 a = 100
i32 b = 50
i32 c = 10
i32 d = 20
    
    // Test all operators
    a += 50   // 150
    b -= 10   // 40
    c *= 5    // 50
    d /= 2    // 10
    
    // Combined result
i32 result = a + b + c + d  // 150 + 40 + 50 + 10 = 250
    
    return result
}
