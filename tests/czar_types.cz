#include <stdio.h>
#include <stdint.h>
#include <stddef.h>

/* Test CZar type replacements */

int main(void) {
    /* Test CZar types - these should be replaced with C types */
    u8 a = 100;
    u16 b = 50000;
    u32 c = 1000000;
    u64 d = 1000000000;

    i8 e = -100;
    i16 f = -30000;
    i32 g = -1000000;
    i64 h = -1000000000;

    f32 i = 3.14f;
    f64 j = 2.718;

    usize k = 42;
    isize l = -42;

    printf("CZar types test: ");
    printf("u8=%u, u16=%u, u32=%u\n", a, b, c);
    printf("i8=%d, i16=%d, i32=%d\n", e, f, g);
    printf("f32=%.2f, f64=%.3f\n", i, j);
    printf("usize=%zu, isize=%zd\n", k, l);
    printf("u64=%llu, i64=%lld\n", (unsigned long long)d, (long long)h);

    printf("CZar type replacement: OK\n");
    return 0;
}
