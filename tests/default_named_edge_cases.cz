// Test edge cases for default and named parameters
fn all_defaults(i32 a = 1, i32 b = 2, i32 c = 3) -> i32 {
    return a + b + c
}

fn some_defaults(i32 a, i32 b = 10, i32 c = 20) -> i32 {
    return a + b + c
}

fn no_defaults(i32 a, i32 b, i32 c) -> i32 {
    return a + b + c
}

fn main() -> i32 {
    // Test calling with all defaults
    i32 r1 = all_defaults()  // 1 + 2 + 3 = 6

    // Test calling with some defaults (first required)
    i32 r2 = some_defaults(5)  // 5 + 10 + 20 = 35

    // Test calling with named arg overriding a default
    i32 r3 = all_defaults(b: 10)  // 1 + 10 + 3 = 14

    // Test calling with multiple named args overriding defaults
    i32 r4 = all_defaults(a: 5, c: 15)  // 5 + 2 + 15 = 22

    // Test positional + named with defaults
    i32 r5 = some_defaults(3, c: 30)  // 3 + 10 + 30 = 43

    // Test all positional (no defaults used)
    i32 r6 = no_defaults(1, 2, 3)  // 1 + 2 + 3 = 6

    // Test all named (no defaults)
    i32 r7 = no_defaults(c: 10, a: 5, b: 7)  // 5 + 7 + 10 = 22

    return r5 - 43
}
