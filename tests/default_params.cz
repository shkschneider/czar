// Test default parameters
fn add(i32 x = 10, i32 y = 20) -> i32 {
    return x + y
}

fn multiply(i32 a, i32 b = 5) -> i32 {
    return a * b
}

fn compute(i32 x = 1, i32 y = 2, i32 z = 3) -> i32 {
    return x + y * z
}

fn main() -> i32 {
    // Call with all defaults
    i32 r1 = add()  // 10 + 20 = 30

    // Call with one argument
    i32 r2 = multiply(6)  // 6 * 5 = 30

    // Call with all arguments
    i32 r3 = add(5, 10)  // 5 + 10 = 15

    // Call with partial defaults
    i32 r4 = compute(2)  // 2 + 2*3 = 8

    return r1 - 30
}
