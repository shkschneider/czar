// Test elseif keyword
fn classify(i32 x) -> i32 {
    if x < 0 {
        return 1  // negative
    } elseif x == 0 {
        return 2  // zero
    } elseif x < 10 {
        return 3  // small positive
    } elseif x < 100 {
        return 4  // medium positive
    } else {
        return 5  // large positive
    }
}

fn main() -> i32 {
    i32 test1 = classify(-5)   // 1
    i32 test2 = classify(0)     // 2
    i32 test3 = classify(5)     // 3
    i32 test4 = classify(50)    // 4
    i32 test5 = classify(500)   // 5

    // Calculate result: 1 + 2 + 3 + 4 + 5 = 15
    return test1 + test2 + test3 + test4 + test5 - 15
}
