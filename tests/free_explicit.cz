// Test explicit free keyword

struct Person {
    i32 age
    i32 score
}

fn main() -> i32 {
    // Heap allocate a Person
    mut Person* p = new Person { age: 25, score: 50 }

    // Use the person
    i32 total = p.age + p.score

    // Explicitly free the memory
    free p

    // Return the total (should be 75)
    return total - 75
}
