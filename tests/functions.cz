// Test function definitions and calls
fn add(i32 a, i32 b) -> i32 {
    return a + b
}

fn multiply(i32 a, i32 b) -> i32 {
    return a * b
}

fn compute(i32 x, i32 y) -> i32 {
    val i32 sum = add(x, y)
    val i32 product = multiply(x, y)
    return sum + product
}

fn main() -> i32 {
    return compute(3, 4)  // (3+4) + (3*4) = 7 + 12 = 19
}
