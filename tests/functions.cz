// Test function definitions and calls
fn add(a: i32, b: i32) -> i32 {
    return a + b
}

fn multiply(a: i32, b: i32) -> i32 {
    return a * b
}

fn compute(x: i32, y: i32) -> i32 {
    sum: i32 = add(x, y)
    product: i32 = multiply(x, y)
    return sum + product
}

fn main() -> i32 {
    return compute(3, 4)  // (3+4) + (3*4) = 7 + 12 = 19
}
