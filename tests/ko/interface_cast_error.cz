// Test that casting to unimplemented interface fails

iface iMyInterface {
    fn tostring() char?
}

iface iMyOtherIface {
    fn toint() i32
}

struct MyStruct : iMyInterface {
    i32 value
}

fn MyStruct:tostring() char? {
    return "MyStruct"
}

fn main() i32 {
    MyStruct s = MyStruct { value: 42 }

    // This should ERROR: s does not implement MyOtherIface
    iMyOtherIface i = <iMyOtherIface> s !!

    return 0
}
