// Test mut keyword with function parameters
struct Counter {
    value: i32
}

fn increment(mut c: *Counter) -> void {
    c.value = c.value + 1
}

fn main() -> i32 {
    mut counter: Counter = Counter { value: 10 }
    increment(&counter)
    increment(&counter)
    increment(&counter)
    return counter.value  // should return 13
}
