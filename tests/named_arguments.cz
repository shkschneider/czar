#include <stdio.h>

/* Test function with multiple parameters */
void my_function(u8 x, u8 y, u8 z) {
    printf("x=%d, y=%d, z=%d\n", x, y, z);
}

/* Another test function */
void test_func(u8 a, u8 b, u8 c, u8 d) {
    printf("a=%d, b=%d, c=%d, d=%d\n", a, b, c, d);
}

u32 main(void) {
    /* Test 1: All positional (no named arguments) */
    my_function(1, 2, 3);
    
    /* Test 2: First argument named */
    my_function(x = 1, 2, 3);
    
    /* Test 3: First two arguments named */
    my_function(x = 1, y = 2, 3);
    
    /* Test 4: All arguments named */
    my_function(x = 1, y = 2, z = 3);
    
    /* Test 5: Last argument named */
    my_function(1, 2, z = 3);
    
    /* Test 6: Mixed usage with more parameters */
    test_func(a = 10, 20, c = 30, d = 40);
    
    /* Test 7: All positional with different values */
    test_func(5, 6, 7, 8);
    
    return 0;
}
