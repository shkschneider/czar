// Test new keyword for heap allocation

struct Person {
    i32 age
    i32 score
}

fn main() -> i32 {
    // Heap allocate a Person struct using new keyword
    Person person = new Person { age: 25, score: 50 }

    // Access fields normally (compiler handles pointer dereferencing)
    i32 total = person.age + person.score

    // Should return 75 (25 + 50)
    return total
}
