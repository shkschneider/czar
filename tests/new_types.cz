// Test new numeric types: i64, u32, u64, f32, f64
fn test_i64() -> i64 {
    i64 x = 1000000000
    i64 y = 2000000000
    return x + y  // 3000000000
}

fn test_u32() -> u32 {
    u32 x = 100
    u32 y = 200
    return x + y  // 300
}

fn test_u64() -> u64 {
    u64 x = 1000
    u64 y = 2000
    return x + y  // 3000
}

fn test_f32() -> f32 {
    f32 x = 1
    f32 y = 2
    return x + y  // 3.0
}

fn test_f64() -> f64 {
    f64 x = 10
    f64 y = 20
    return x + y  // 30.0
}

fn main() -> i32 {
    i64 a = test_i64()
    u32 b = test_u32()
    u64 c = test_u64()
    f32 d = test_f32()
    f64 e = test_f64()

    // Return sum of type tests (truncated for i32)
    // Just verify they compile and basic math works
    return 1  // success
}
