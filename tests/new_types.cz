// Test new numeric types: i64, u32, u64, f32, f64
fn test_i64() -> i64 {
    val i64 x = 1000000000
    val i64 y = 2000000000
    return x + y  // 3000000000
}

fn test_u32() -> u32 {
    val u32 x = 100
    val u32 y = 200
    return x + y  // 300
}

fn test_u64() -> u64 {
    val u64 x = 1000
    val u64 y = 2000
    return x + y  // 3000
}

fn test_f32() -> f32 {
    val f32 x = 1
    val f32 y = 2
    return x + y  // 3.0
}

fn test_f64() -> f64 {
    val f64 x = 10
    val f64 y = 20
    return x + y  // 30.0
}

fn main() -> i32 {
    val i64 a = test_i64()
    val u32 b = test_u32()
    val u64 c = test_u64()
    val f32 d = test_f32()
    val f64 e = test_f64()
    
    // Return sum of type tests (truncated for i32)
    // Just verify they compile and basic math works
    return 1  // success
}
