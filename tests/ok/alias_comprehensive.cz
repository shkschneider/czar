// Comprehensive test for #alias macro
import cz

// Test struct type alias
struct Point {
    i32 x
    i32 y
}

#alias Pt Point

// Test function alias with module prefix
#alias print cz.print
#alias println cz.println

fn main() i32 {
    // Test struct type alias
    Pt p = Pt { x: 10, y: 20 }
    
    // Test function alias
    print("Point: ")
    println("")
    
    if p.x == 10 and p.y == 20 {
        return 0
    }
    
    return 1
}
