// Test complex boolean expressions
fn main() i32 {
    bool a = true
    bool b = false
    bool c = true
    
    // Test various boolean combinations
    bool r1 = a && b || c       // false || true = true
    bool r2 = a && (b || c)     // true && true = true
    bool r3 = !a || b           // false || false = false
    bool r4 = !(a && b)         // !false = true
    bool r5 = a && !b && c      // true && true && true = true
    
    i32 count = 0
    if r1 { count = count + 1 }  // +1
    if r2 { count = count + 1 }  // +1
    if r3 { count = count + 1 }  // no change
    if r4 { count = count + 1 }  // +1
    if r5 { count = count + 1 }  // +1
    
    return count - 4  // 4 true values
}
