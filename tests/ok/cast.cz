// Test '<Type>' unsafe cast functionality

fn main() i32 {
    // Test basic casting
    i32 num = 42
    bool b = <bool> num !!  // non-zero should be true

    // Test numeric casts
    i32 small = 100
    i64 big = <i64> small
    i32 back = <i32> big !!

    // Test with expressions
    i32 result = <i32> 10 !! + 20

    if b {
        if back == 100 {
            if result == 30 {
                return 0  // success
            }
        }
    }

    return 30
}
