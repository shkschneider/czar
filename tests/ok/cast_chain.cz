// Test casting between different numeric types
fn main() i32 {
    // Test various casts
    i32 i = 42
    u32 u = <u32> i !!
    i64 l = <i64> u !!
    i8 b = <i8> 127 !!
    u16 s = <u16> b !!
    
    // Cast back to i32 for comparison
    i32 result = <i32> s !!
    
    return result - 127
}
