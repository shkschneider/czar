// Test new cast syntax with <Type> expr
#module main

fn main() i32 {
    // Safe widening cast - no error, no warning
    u8 a = <u8> 10 !!
    u16 b = <u16> a
    u32 c = <u32> b
    
    // Unsafe narrowing cast with !! - warning but compiles
    u16 d = <u16> 300 !!
    u8 e = <u8> d !!
    
    // Safe cast with fallback (fallback is implicitly converted)
    i32 f = 100
    u8 g = <u8> f ?? 255
    
    return 0
}
