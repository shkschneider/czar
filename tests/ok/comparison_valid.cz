// Test: Valid comparison operations
// Should compile successfully

struct Point {
    i32 x
    i32 y
}

fn main() i32 {
    // Struct pointer comparisons (nullable types)
    Point? ptr1 = null
    Point? ptr2 = null
    bool ptr_eq = (ptr1 == ptr2)
    bool ptr_ne = (ptr1 != ptr2)
    
    // Numeric comparisons
    i32 a = 10
    i32 b = 20
    bool less = (a < b)
    bool greater = (a > b)
    bool le = (a <= b)
    bool ge = (a >= b)
    bool eq = (a == b)
    
    // Numeric widening comparison
    i32 small = 10
    i64 large = 20
    bool mixed = (small < large)
    
    // Bool comparisons
    bool t = true
    bool f = false
    bool bool_eq = (t == f)
    
    return 0
}
