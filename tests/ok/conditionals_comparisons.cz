// Test conditionals with comparison operators

fn compare_values(i32 a, i32 b) i32 {
    if a == b {
        return 0  // equal
    }
    if a > b {
        return 1  // greater
    }
    if a < b {
        return -1  // less
    }
    return 99  // should never reach
}

fn is_in_range(i32 value, i32 min, i32 max) bool {
    if value >= min and value <= max {
        return true
    }
    return false
}

fn main() i32 {
    // Test equality
    if compare_values(10, 10) != 0 {
        return 1
    }
    
    // Test greater than
    if compare_values(20, 10) != 1 {
        return 2
    }
    
    // Test less than
    if compare_values(5, 15) != -1 {
        return 3
    }
    
    // Test range checking
    if not is_in_range(50, 0, 100) {
        return 4
    }
    
    if is_in_range(-5, 0, 100) {
        return 5
    }
    
    if is_in_range(150, 0, 100) {
        return 6
    }
    
    // Test edge cases
    if not is_in_range(0, 0, 100) {
        return 7
    }
    
    if not is_in_range(100, 0, 100) {
        return 8
    }
    
    return 0
}
