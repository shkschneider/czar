// Test conditionals with logical operators (and, or, not)

fn is_valid_age(i32 age) bool {
    if age >= 0 and age <= 120 {
        return true
    }
    return false
}

fn is_teen_or_adult(i32 age) bool {
    if age >= 13 and age < 20 {
        return true  // teen
    }
    if age >= 20 {
        return true  // adult
    }
    return false
}

fn is_special_age(i32 age) bool {
    // Special ages: 18, 21, 30, 40, 50, 60, 70, 80, 90, 100
    if age == 18 or age == 21 or age == 30 {
        return true
    }
    if age == 40 or age == 50 or age == 60 {
        return true
    }
    if age == 70 or age == 80 or age == 90 or age == 100 {
        return true
    }
    return false
}

fn main() i32 {
    // Test valid ages
    if not is_valid_age(25) {
        return 1
    }
    
    if is_valid_age(-5) {
        return 2
    }
    
    if is_valid_age(150) {
        return 3
    }
    
    // Test teen or adult
    if not is_teen_or_adult(15) {
        return 4
    }
    
    if not is_teen_or_adult(25) {
        return 5
    }
    
    if is_teen_or_adult(10) {
        return 6
    }
    
    // Test special ages
    if not is_special_age(21) {
        return 7
    }
    
    if not is_special_age(50) {
        return 8
    }
    
    if is_special_age(35) {
        return 9
    }
    
    return 0
}
