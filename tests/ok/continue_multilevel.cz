// Test multi-level continue
fn test_continue_2_levels() i32 {
    mut i32 sum = 0
    mut i32 outer = 0
    
    repeat 4 {
        outer = outer + 1
        mut i32 inner = 0
        repeat 4 {
            inner = inner + 1
            if outer == 2 and inner == 2 {
                continue 2  // Skip to next outer iteration
            }
            sum = sum + 1
        }
    }
    
    return sum  // 4 + 1 + 4 + 4 = 13 (when outer==2, inner==2, continues outer)
}

fn test_continue_in_nested() i32 {
    mut i32 count = 0
    mut i32 i = 0
    
    while i < 3 {
        i = i + 1
        mut i32 j = 0
        repeat 3 {
            j = j + 1
            if i == 2 and j == 1 {
                continue 2  // Continue outer while loop
            }
            count = count + 1
        }
    }
    
    return count  // 3 + 0 + 3 = 6 (when i==2, j==1, continues outer)
}

fn main() i32 {
    i32 a = test_continue_2_levels()    // 13
    i32 b = test_continue_in_nested()   // 6
    
    return (a + b) - 19  // 13 + 6 = 19
}
