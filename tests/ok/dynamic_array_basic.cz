// Test basic dynamic array allocation with new [...]
fn main() i32 {
    // Create a dynamic array on the heap
    i32[] numbers = new [1, 2, 3, 4, 5]
    
    // Access elements
    i32 sum = numbers[0] + numbers[1] + numbers[2] + numbers[3] + numbers[4]
    
    // Expected: 1 + 2 + 3 + 4 + 5 = 15
    return sum - 15
}
