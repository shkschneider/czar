// Test dynamic array with struct elements
struct Point {
    i32 x
    i32 y
}

fn main() i32 {
    // Create dynamic array of Points
    Point[] points = new [
        Point { x: 1, y: 2 },
        Point { x: 3, y: 4 },
        Point { x: 5, y: 6 }
    ]
    
    // Sum all coordinates
    i32 sum = points[0].x + points[0].y + 
              points[1].x + points[1].y + 
              points[2].x + points[2].y
    
    // Expected: 1 + 2 + 3 + 4 + 5 + 6 = 21
    return sum - 21
}
