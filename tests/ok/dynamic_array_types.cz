// Test dynamic array with different element types
fn main() i32 {
    // u8 dynamic array
    u8[] bytes = new [<u8> 10 !!, <u8> 20 !!, <u8> 30 !!]
    
    // bool dynamic array
    bool[] flags = new [true, false, true, true]
    
    // Calculate result
    i32 byte_sum = bytes[0] as<i32> + bytes[1] as<i32> + bytes[2] as<i32>
    
    mut i32 flag_count = 0
    if flags[0] {
        flag_count = flag_count + 1
    }
    if flags[1] {
        flag_count = flag_count + 1
    }
    if flags[2] {
        flag_count = flag_count + 1
    }
    if flags[3] {
        flag_count = flag_count + 1
    }
    
    // Expected: (10 + 20 + 30) + (1 + 0 + 1 + 1) = 60 + 3 = 63
    return (byte_sum + flag_count) - 63
}
