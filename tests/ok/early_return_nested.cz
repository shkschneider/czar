// Test early return from nested conditionals
fn classify(i32 x) i32 {
    if x < 0 {
        if x < -10 {
            return 1
        }
        return 2
    } elseif x == 0 {
        return 3
    } else {
        if x > 10 {
            return 4
        }
        return 5
    }
}

fn main() i32 {
    i32 r1 = classify(-20)  // 1
    i32 r2 = classify(-5)   // 2
    i32 r3 = classify(0)    // 3
    i32 r4 = classify(20)   // 4
    i32 r5 = classify(5)    // 5
    
    return r1 + r2 + r3 + r4 + r5 - 15  // 1+2+3+4+5 = 15
}
