// Test basic function overloading with different types
#module main

fn add(u8 a, u8 b) u8 {
    return a + b
}

fn add(i32 a, i32 b) i32 {
    return a + b
}

fn add(f32 a, f32 b) f32 {
    return a + b
}

fn main() i32 {
    u8 x = add(<u8> 1 !!, <u8> 2 !!)
    i32 y = add(10, 20)
    f32 z = add(<f32> 1 !!, <f32> 2 !!)
    
    // Verify correct values
    if x != <u8> 3 !! {
        return 1
    }
    if y != 30 {
        return 2
    }
    if z < <f32> 2 !! or z > <f32> 4 !! {
        return 3
    }
    
    return 0
}
