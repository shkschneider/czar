// Test basic generics with single type parameter
module main

// Generic function for unsigned integers
fn <u8:u16:u32:u64> add(T a, T b) T {
    return a + b
}

// Generic function for signed integers
fn <i8:i16:i32:i64> max(T a, T b) T {
    if a > b {
        return a
    }
    return b
}

fn main() i32 {
    // Test u8 variant
    u8 a = 5 as<u8>
    u8 b = 10 as<u8>
    u8 sum1 = add(a, b)
    
    // Test u32 variant
    u32 c = 100 as<u32>
    u32 d = 200 as<u32>
    u32 sum2 = add(c, d)
    
    // Test i32 variant
    i32 e = 50
    i32 f = 30
    i32 max_val = max(e, f)
    
    if sum1 != 15 as<u8> {
        return 1
    }
    if sum2 != 300 as<u32> {
        return 2
    }
    if max_val != 50 {
        return 3
    }
    
    return 0
}
