// Test basic generics with single type parameter
#module main

// Generic function for unsigned integers
fn <u8:u16:u32:u64> add(T a, T b) T {
    return a + b
}

// Generic function for signed integers
fn <i8:i16:i32:i64> max(T a, T b) T {
    if a > b {
        return a
    }
    return b
}

fn main() i32 {
    // Test u8 variant
    u8 a = <u8> 5 !!
    u8 b = <u8> 10 !!
    u8 sum1 = add(a, b)
    
    // Test u32 variant
    u32 c = <u32> 100 !!
    u32 d = <u32> 200 !!
    u32 sum2 = add(c, d)
    
    // Test i32 variant
    i32 e = 50
    i32 f = 30
    i32 max_val = max(e, f)
    
    if sum1 != <u8> 15 !! {
        return 1
    }
    if sum2 != <u32> 300 !! {
        return 2
    }
    if max_val != 50 {
        return 3
    }
    
    return 0
}
