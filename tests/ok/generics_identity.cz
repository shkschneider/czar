// Test generic function with multiple calls using different types

// Generic identity function
fn <i8:i16:i32:i64> identity(T x) T {
    return x
}

// Generic function for triple
fn <i32:i64> triple(T n) T {
    return n + n + n
}

fn main() i32 {
    // Test identity with i32
    i32 a = identity(100)
    if a != 100 {
        return 1
    }
    
    i32 b = identity(-50)
    if b != -50 {
        return 2
    }
    
    // Test identity with i16
    i16 c = identity(<i16> 200 !!)
    if c != <i16> 200 !! {
        return 3
    }
    
    // Test triple
    i32 tripled = triple(10)
    if tripled != 30 {
        return 4
    }
    
    i32 neg_tripled = triple(-7)
    if neg_tripled != -21 {
        return 5
    }
    
    // Test with i64
    i64 big_tripled = triple(<i64> 100 !!)
    if big_tripled != <i64> 300 !! {
        return 6
    }
    
    return 0
}
