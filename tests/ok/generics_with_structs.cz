// Test generics with regular functions and different operations

fn <i32:i64> get_value(T val) T {
    return val
}

fn <i32:i64> add_values(T a, T b) T {
    return a + b
}

fn <i32:i64> subtract_values(T a, T b) T {
    return a - b
}

fn main() i32 {
    // Simple generic function calls
    i32 x = get_value(42)
    if x != 42 {
        return 1
    }
    
    i32 sum = add_values(10, 20)
    if sum != 30 {
        return 2
    }
    
    // Test with negative numbers
    i32 neg_sum = add_values(-5, -10)
    if neg_sum != -15 {
        return 3
    }
    
    // Test mixed signs
    i32 mixed = add_values(50, -20)
    if mixed != 30 {
        return 4
    }
    
    // Test subtraction
    i32 diff = subtract_values(100, 40)
    if diff != 60 {
        return 5
    }
    
    // Test with i64
    i64 big_val = get_value(<i64> 1000 !!)
    if big_val != <i64> 1000 !! {
        return 6
    }
    
    return 0
}
