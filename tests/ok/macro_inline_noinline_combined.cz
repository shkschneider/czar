// Test both #inline and #noinline together
// Verifies that both directives can be used in the same file

fn #inline fast_add(i32 a, i32 b) i32 {
    return a + b
}

fn #noinline complex_calculation(i32 x) i32 {
    mut i32 result = 0
    mut i32 i = 0
    while i < x {
        result = fast_add(result, i)
        i = i + 1
    }
    return result
}

fn main() i32 {
    i32 result = complex_calculation(10)
    return 0
}
