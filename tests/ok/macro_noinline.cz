// Test #noinline directive
// Verifies that #noinline generates proper C noinline attributes

fn #noinline calculate(i32 x) i32 {
    return x * x + x
}

fn #noinline process(i32 a, i32 b) i32 {
    return calculate(a) + calculate(b)
}

fn main() i32 {
    i32 result = process(3, 4)
    return 0
}
