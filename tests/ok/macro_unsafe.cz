// Test #unsafe block
#import cz.fmt
// Verifies that raw C code can be embedded and passed through

fn test_unsafe() i32 {
    mut i32 x = 0

    #unsafe {
        x = 42;
    }
    
    // Use fmt.printf instead of raw C printf
    fmt.printf("Value from unsafe block: %d\n", x)

    return 0
}

fn main() i32 {
    return test_unsafe()
}
