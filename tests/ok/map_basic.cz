// Test basic map type with key-value pairs using new syntax
fn main() i32 {
    // Create a map with new syntax
    map<i32:i32> numbers = new map {
        1: 10,
        2: 20,
        3: 30
    }
    
    // Access map elements
    i32 sum = numbers.values[0] + numbers.values[1] + numbers.values[2]
    
    // Expected: 10 + 20 + 30 = 60
    free numbers
    return sum - 60
}
