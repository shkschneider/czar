// Test map with comprehensive features
fn main() i32 {
    // Create a map with bool values
    i32{bool} flags = new {
        1: true,
        2: false,
        3: true
    }
    
    // Access map properties
    i32 flag_count = flags.size
    
    // Verify the size
    if flag_count != 3 {
        return 2
    }
    
    // Check values individually
    bool first = flags.values[0]
    bool second = flags.values[1]
    bool third = flags.values[2]
    
    if first != true {
        return 3
    }
    
    if second != false {
        return 4
    }
    
    if third != true {
        return 5
    }
    
    return 0  // Success
}
