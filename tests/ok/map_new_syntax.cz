// Test new map syntax
fn main() i32 {
    // Create a map with new simplified syntax
    [i32]i32 numbers = new {
        1: 10,
        2: 20,
        3: 30
    }
    
    // Access map elements
    i32 sum = numbers.values[0] + numbers.values[1] + numbers.values[2]
    
    // Expected: 10 + 20 + 30 = 60
    return sum - 60
}
