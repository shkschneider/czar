// Test multiple method calls chained
struct Counter {
    i32 value
}

fn Counter:increment() void {
    self.value = self.value + 1
}

fn Counter:add(i32 n) void {
    self.value = self.value + n
}

fn Counter:get() i32 {
    return self.value
}

fn main() i32 {
    mut Counter c = Counter { value: 0 }
    
    c:increment()
    c:increment()
    c:add(5)
    c:increment()
    
    return c:get() - 8  // 0+1+1+5+1 = 8
}
