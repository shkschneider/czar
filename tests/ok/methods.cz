// Test method syntax
struct Vec2 {
    i32 x
    i32 y
}

fn Vec2:add(i32 dx, i32 dy) void {
    self.x = self.x + dx
    self.y = self.y + dy
}

fn Vec2:sum() i32 {
    return self.x + self.y
}

// Static method that takes a Vec2
fn Vec2.add_static(Vec2 v, i32 dx, i32 dy) i32 {
    return v.x + dx + v.y + dy
}

fn main() i32 {
    mut Vec2 v = Vec2 { x: 10, y: 20 }
    
    // Method call
    v:add(5, 3)
    
    // Static method call
    i32 result = Vec2.add_static(v, 1, 2)
    
    // v should now be {15, 23}
    // result should be 15 + 1 + 23 + 2 = 41
    if v:sum() == 38 {
        if result == 41 {
            return 0
        }
    }
    
    return 1
}
