// Test mixed pointer and value parameters
struct Value {
    i32 data
}

fn by_value(Value v) i32 {
    return v.data
}

fn by_pointer(Value* v) i32 {
    return v.data
}

fn mixed(Value v, Value* p) i32 {
    return v.data + p.data
}

fn main() i32 {
    Value v1 = Value { data: 10 }
    Value v2 = Value { data: 20 }
    
    i32 r1 = by_value(v1)
    i32 r2 = by_pointer(&v2)
    i32 r3 = mixed(v1, &v2)
    
    return r1 + r2 + r3 - 60  // 10 + 20 + 30 = 60
}
