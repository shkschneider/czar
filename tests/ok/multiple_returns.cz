// Test multiple return paths
fn classify_number(i32 n) i32 {
    if n < 0 {
        return -1
    }
    
    if n == 0 {
        return 0
    }
    
    if n < 10 {
        return 1
    }
    
    if n < 100 {
        return 2
    }
    
    return 3
}

fn main() i32 {
    i32 r1 = classify_number(-5)   // -1
    i32 r2 = classify_number(0)    // 0
    i32 r3 = classify_number(5)    // 1
    i32 r4 = classify_number(50)   // 2
    i32 r5 = classify_number(500)  // 3
    
    // Sum: -1 + 0 + 1 + 2 + 3 = 5
    return r1 + r2 + r3 + r4 + r5 - 5
}
