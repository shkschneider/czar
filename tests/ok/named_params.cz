// Test named parameters
fn add(i32 x, i32 y) i32 {
    return x + y
}

fn subtract(i32 a, i32 b) i32 {
    return a - b
}

fn compute(i32 x, i32 y, i32 z) i32 {
    return x + y - z
}

fn main() i32 {
    // Call with named parameters in order
    i32 r1 = add(x: 10, y: 20)  // 10 + 20 = 30

    // Call with named parameters out of order
    i32 r2 = add(y: 20, x: 22)  // 22 + 20 = 42

    // Call with mixed positional and named
    i32 r3 = subtract(50, b: 8)  // 50 - 8 = 42

    // Call with all named
    i32 r4 = compute(z: 10, y: 20, x: 32)  // 32 + 20 - 10 = 42

    return r2 - 42
}
