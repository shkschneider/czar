// Test nested struct hierarchies (3+ levels deep)
struct Inner {
    i32 value
}

struct Middle {
    Inner inner
    i32 count
}

struct Outer {
    Middle middle
    i32 total
}

fn main() i32 {
    Outer o = Outer {
        middle: Middle {
            inner: Inner { value: 5 },
            count: 10
        },
        total: 15
    }
    
    // Access deeply nested field
    i32 v = o.middle.inner.value
    i32 c = o.middle.count
    i32 t = o.total
    
    return v + c + t - 30  // 5 + 10 + 15 = 30
}
