// Test new map syntax with explicit map keyword
fn main() i32 {
    // Test heap-allocated map with new keyword
    map<i32:bool>* m1 = new map { 1: true, 2: false }
    
    // Access map properties
    i32 size = m1.size
    
    if size != 2 {
        return 1
    }
    
    // Access values
    bool val1 = m1.values[0]
    bool val2 = m1.values[1]
    
    if val1 != true {
        return 2
    }
    if val2 != false {
        return 3
    }
    
    free m1
    return 0
}
