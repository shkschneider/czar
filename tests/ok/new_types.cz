// Test new numeric types: i64, u32, u64, f32, f64
fn test_i64() i64 {
    i64 x = 1000000000 as<i64>
    i64 y = 2000000000 as<i64>
    return x + y  // 3000000000
}

fn test_u32() u32 {
    u32 x = 100 as<u32>
    u32 y = 200 as<u32>
    return x + y  // 300
}

fn test_u64() u64 {
    u64 x = 1000 as<u64>
    u64 y = 2000 as<u64>
    return x + y  // 3000
}

fn test_f32() f32 {
    f32 x = 1 as<f32>
    f32 y = 2 as<f32>
    return x + y  // 3.0
}

fn test_f64() f64 {
    f64 x = 10 as<f64>
    f64 y = 20 as<f64>
    return x + y  // 30.0
}

fn main() i32 {
    i64 a = test_i64()
    u32 b = test_u32()
    u64 c = test_u64()
    f32 d = test_f32()
    f64 e = test_f64()

    // Return sum of type tests (truncated for i32)
    // Just verify they compile and basic math works
    return 0 // success
}
