// Test safe pointer casting scenarios

struct Vec2 {
    i32 x
    i32 y
}

struct Vec3 {
    i32 x
    i32 y
    i32 z
}

fn print_vec2(Vec2? v) void {
    // Just access to verify type
    mut i32 _ = v.x + v.y
}

fn main() i32 {
    // Create structs
    Vec2 v2 = Vec2 { x: 1, y: 2 }
    Vec3 v3 = Vec3 { x: 3, y: 4, z: 5 }

    // Convert to any (implicit cast)
    any a2 = v2
    any a3 = v3

    // Create nullable pointers
    Vec2? ptr2 = v2
    Vec3? ptr3 = v3

    // Call function with nullable
    print_vec2(ptr2)

    return 0
}
