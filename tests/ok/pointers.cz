// Test explicit pointers and address-of operator
struct Vec2 {
    i32 x
    i32 y
}

fn not_modify(Vec2 p) void {
    // Cannot modify - receives value (copy)
    // p.x = 10
    // p.y = 20
}

fn modify(mut Vec2? p) void {
    p.x = 10
    p.y = 20
}

fn main() i32 {
    mut Vec2 v = Vec2 { x: 1, y: 2 }
    not_modify(v)
    modify(mut v)
    return v.x + v.y - 30
}
