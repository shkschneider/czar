// Comprehensive test of all print functions
#import cz.fmt

fn main() i32 {
    // Test println
    fmt.println("=== Testing println ===")
    fmt.println("Line 1")
    fmt.println("Line 2")

    // Test print
    fmt.print("=== Testing print ===")
    fmt.print("\n")
    fmt.print("A")
    fmt.print("B")
    fmt.print("C")
    fmt.println("")

    // Test printf with various types
    fmt.println("=== Testing printf ===")
    i32 age = 25
    i32 year = 2024
    fmt.printf("Age: %d, Year: %d\n", age, year)

    i32 a = 10
    i32 b = 20
    i32 c = 30
    fmt.printf("Values: %d, %d, %d\n", a, b, c)

    // Printf without arguments
    fmt.printf("Done!\n")

    return 0
}
