// Test pub visibility modifier on structs
#module main

pub struct Point {
    i32 x
    i32 y
}

struct InternalData {
    i32 value
}

pub fn create_point(i32 x, i32 y) Point {
    Point p = Point { x: x, y: y }
    return p
}

fn main() i32 {
    Point p = create_point(10, 20)
    return p.x + p.y - 30
}
