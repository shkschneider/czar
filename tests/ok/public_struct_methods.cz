// Test: Public struct with methods of different visibilities
// Expected behavior:
// - method1 (no modifier) should be public by default
// - method2 (prv modifier) should be private

pub struct PublicStruct {
    i32 value
}

fn PublicStruct:method1() i32 {
    return self.value
}

prv fn PublicStruct:method2() i32 {
    return self.value * 2
}

pub fn PublicStruct:get_double() i32 {
    // Private method can be called from within methods
    return self:method2()
}

fn main() i32 {
    PublicStruct ps = PublicStruct { value: 5 }
    // Public method should work
    i32 v1 = ps:method1()
    // Public method calling private method should work
    i32 v2 = ps:get_double()
    if v1 == 5 and v2 == 10 {
        return 0
    }
    return 1
}
