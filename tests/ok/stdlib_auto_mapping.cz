// Test automatic mapping of stdlib functions
// This demonstrates that new functions can be added to stdlib modules
// without requiring manual hardcoding in calls.lua

#import cz.fmt

fn main() i32 {
    // Static method syntax - works automatically for all stdlib functions
    fmt.println("Testing auto-mapped println")
    fmt.print("Auto-mapped print")
    fmt.printf(" with printf\n")
    
    // New functions added to stdlib work automatically
    fmt.debug("Auto-mapped debug function")
    
    return 0
}
