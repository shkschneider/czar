// Test String:append() method
#import cz.fmt

fn main() i32 {
    String? s = new String { "Hello" }
    String s2 = String { " World" }

    // Append to the String
    s:append(s2)

    // Check new length
    if s.length != 11 {
        return 1
    }

    // Verify content
    fmt.print(s:cstr())

    free s
    return 0
}
