// Test string interpolation with mustache-like syntax
#import cz
fn main() i32 {
    i32 age = 25
    i32 year = 2024
    
    // Test basic interpolation
    cz.println("Hello {age}")
    
    // Test multiple interpolations
    cz.println("Age: {age}, Year: {year}")
    
    // Test escaped braces
    cz.print("Not interpolated: \{age\}")
    cz.println("")
    
    // Test with expressions
    cz.println("Next year: {year + 1}")
    
    // Test with print (no newline)
    cz.print("Value: {age}")
    cz.println("")
    
    return 0
}
