// Test string:prefix() method
fn main() i32 {
    string s = string { "Hello World" }
    string prefix1 = string { "Hello" }
    string prefix2 = string { "World" }
    
    // Check if string starts with "Hello"
    i32 has_prefix = s.prefix(prefix1)
    
    if has_prefix != 1 {
        return 1
    }
    
    // Check if string starts with "World" (should not)
    i32 no_prefix = s.prefix(prefix2)
    
    if no_prefix != 0 {
        return 2
    }
    
    // Test empty prefix (should always match)
    string empty = string { "" }
    i32 empty_prefix = s.prefix(empty)
    
    if empty_prefix != 1 {
        return 3
    }
    
    // Test prefix longer than string
    string long_prefix = string { "Hello World Extended" }
    i32 long_no_match = s.prefix(long_prefix)
    
    if long_no_match != 0 {
        return 4
    }
    
    return 0
}
