// Test string:trim() methods
fn main() i32 {
    // Test ltrim
    String? s1 = new String { "   hello" }
    s1:ltrim()
    if s1.length != 5 {
        return 1
    }
    
    // Test rtrim
    String? s2 = new String { "world   " }
    s2:rtrim()
    if s2.length != 5 {
        return 2
    }
    
    // Test trim (both sides)
    String? s3 = new String { "  test  " }
    s3:trim()
    if s3.length != 4 {
        return 3
    }
    
    free s1
    free s2
    free s3
    return 0
}
