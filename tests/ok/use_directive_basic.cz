// Test that importing a module gives direct access to the last component
// #import cz.fmt gives access to fmt.print(), not cz.fmt.print()
#import cz.fmt

fn main() i32 {
    // Can call functions using the last component of the import path
    fmt.print("Imports automatically scope to last component!\n")
    fmt.println("No #use directive needed")
    return 0
}
