// Test that valid nullable operations are still allowed
struct Node {
    i32 value
}

fn main() i32 {
    mut Node? p = new Node { value: 10 }
    
    // Assignment through nullable is allowed with mut
    p.value = 20
    
    // Null check is allowed
    if p != null {
        return 0
    }
    
    return 1
}
