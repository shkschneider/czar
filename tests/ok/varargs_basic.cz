// Test basic varargs functionality
fn sum(i32... nums) i32 {
    mut i32 total = 0
    mut i32 i = 0
    while i < 3 {
        total = total + nums[i]
        i = i + 1
    }
    return total
}

fn main() i32 {
    i32 result = sum(10, 20, 30)
    // Should return 60 (10 + 20 + 30)
    return result - 60
}
