// Test varargs with different types
fn sum_u8(u8... nums) u8 {
    mut u8 total = <u8> 0 !!
    mut i32 i = 0
    while i < 3 {
        total = total + nums[i]
        i = i + 1
    }
    return total
}

fn main() i32 {
    u8 result = sum_u8(10, 20, 30)
    // Should return 60
    return <i32> result !! - 60
}
