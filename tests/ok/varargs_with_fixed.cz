// Test varargs with fixed parameters
fn compute(i32 a, i32 b, i32... rest) i32 {
    mut i32 total = a + b
    mut i32 i = 0
    while i < 2 {
        total = total + rest[i]
        i = i + 1
    }
    return total
}

fn main() i32 {
    i32 result = compute(10, 20, 5, 5)
    // Should return 40 (10 + 20 + 5 + 5)
    return result - 40
}
