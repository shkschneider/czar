#include "cz/runtime.h"
#include <stdio.h>

/* Test that cz_runtime.h can be included independently */

void test_function(void) {
    printf("Testing from %s at line %d in %s\n", FILE, LINE, FUNC);
}

int main(void) {
    test_function();

    cz_assert(1 == 1);
    cz_assert(2 + 2 == 4);

    printf("cz_runtime.h: OK\n");
    return 0;
}
