// Test arithmetic operators
fn main() -> i32 {
    val a: i32 = 10
    val b: i32 = 3
    
    val sum: i32 = a + b        // 13
    val diff: i32 = a - b       // 7
    val prod: i32 = a * b       // 30
    val quot: i32 = a / b       // 3
    
    return sum + diff + prod + quot  // 13 + 7 + 30 + 3 = 53
}
