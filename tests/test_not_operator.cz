// Test the new 'not' operator
fn main() i32 {
    bool a = true
    bool b = false
    
    // Test not operator
    bool not_a = not a  // should be false
    bool not_b = not b  // should be true
    
    // Test in conditions
    if not a {
        return 1  // should not execute
    }
    
    if not b {
        return 0  // should execute - success
    }
    
    return 2  // should not reach here
}
