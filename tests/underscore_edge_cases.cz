// Additional underscore tests - edge cases

fn all_unused(_: i32, _: i32, _: bool) -> i32 {
    return 100
}

fn mixed_params(a: i32, _: bool, b: i32, _: i32) -> i32 {
    return a + b
}

struct Point {
    x: i32
    y: i32
}

fn Point.ignore_self(_: *Point) -> i32 {
    return 42
}

fn main() -> i32 {
    // All parameters unused
    val v1: i32 = all_unused(1, 2, true)
    
    // Mixed used and unused
    val v2: i32 = mixed_params(10, false, 20, 99)
    
    // Method with underscore self
    var p: Point = Point { x: 5, y: 10 }
    val v3: i32 = p.ignore_self()
    
    // Multiple discard statements
    _ = all_unused(1, 2, false)
    _ = mixed_params(5, true, 5, 0)
    
    return v1 + v2 + v3  // 100 + 30 + 42 = 172
}
