// Additional underscore tests - edge cases

fn all_unused(i32 _, i32 _, bool _) -> i32 {
    return 100
}

fn mixed_params(i32 a, bool _, i32 b, i32 _) -> i32 {
    return a + b
}

struct Point {
    i32 x
    i32 y
}

fn Point.ignore_self(Point _) -> i32 {
    return 42
}

fn main() -> i32 {
    // All parameters unused
    i32 v1 = all_unused(1, 2, true)

    // Mixed used and unused
    i32 v2 = mixed_params(10, false, 20, 99)

    // Method with underscore self
    Point p = Point { x: 5, y: 10 }
    i32 v3 = Point.ignore_self(p)

    // Multiple discard statements
    _ = all_unused(1, 2, false)
    _ = mixed_params(5, true, 5, 0)

    return v1 + v2 + v3  // 100 + 30 + 42 = 172
}
