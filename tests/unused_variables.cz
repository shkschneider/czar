/*
 * Test the special _ variable for suppressing unused warnings
 * Multiple _ variables can exist in the same scope
 */

#include <stdint.h>
#include <stdio.h>
#include "cz.h"

int main(void) {
    /* These would normally generate "unused variable" warnings */
    u8 _ = 10;
    u16 _ = 20;
    u32 _ = 30;
    u64 _ = 40;
    
    /* But we can still use regular variables */
    i32 used_var = 100;
    
    /* More unused variables with different scoping */
    {
        f32 _ = 3.14f;
        f64 _ = 2.718;
    }
    
    /* Use the regular variable to avoid warning */
    ASSERT(used_var == 100);
    
    printf("Unused variable test passed!\n");
    return 0;
}
