// Comprehensive test with multiple patterns
fn classify(i32 n) i32 {
    if n == 0 {
        return 1
    } elseif n == 1 {
        return 2
    } elseif n == 2 {
        return 0
    } elseif n == 3 {
        return 3
    } else {
        return 4
    }
}

fn check_range(i32 val) i32 {
    if val < 0 {
        return 1
    } elseif val == 0 {
        return 2
    } elseif val < 10 {
        return 3
    } elseif val < 100 {
        return 0
    } else {
        return 4
    }
}

fn main() i32 {
    i32 a = classify(2)
    i32 b = check_range(50)
    return (a + b) - 0
}
