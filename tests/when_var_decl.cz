// Test with variable declaration
struct Point {
    i32 x
    i32 y
}

fn test_var_decl() i32 {
    Point p = Point { x: 5, y: 10 }
    i32 val = p.x

    if val == 1 {
        return 1
    } elseif val == 5 {
        return 0
    } else {
        return 2
    }
}

fn main() i32 {
    return test_var_decl()
}
